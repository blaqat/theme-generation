	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#c4c4c4"
			}
		},
		{
			"scope": ["string"],
			"settings": {
				"foreground": "#c8ffa7"
			}
		},
		{
			"scope": ["storage.type.function.arrow"],
			"settings": {
				"foreground": "#FFFFFFD8"
			}
		},
		{
			"scope": ["constant.other"],
			"settings": {
				"foreground": "#b181ec"
			}
		},
		{
			"scope": [
				"storage.type",
				"storage.modifier",
				"keyword",
				"variable.language.this",
				"variable.language.super",
				"keyword.other.typedef",
				"constant.language"
			],
			"settings": {
				"foreground": "#FE7AB2"
			}
		},
		{
			"scope": ["entity.name.type.class"],
			"settings": {
